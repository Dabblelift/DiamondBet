@model DiamondBet.Web.ViewModels.Teams.TeamsListViewModel;
@{
    this.ViewData["Title"] = "All Teams";
}

<h1>@this.ViewData["Title"]</h1>


<a class="nav-link text-dark" asp-area="" asp-controller="Teams" asp-action="Add">Add a New Team</a>


@foreach (var team in Model.Teams)
{
    <li>@team.Name ~ (@team.CountryName)</li>
    <a class="nav-link text-dark" asp-area="" asp-controller="Teams" asp-action="ById" asp-route-id="@team.Id">Go to Team Page</a>
}
